  "description": "Know the Game API",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "start": "npx sequelize-cli db:migrate && babel-node src/index.js",
    "format": "eslint --fix \"**/*.js\" && prettier --write \"src/**/*.js\"",
    "lint": "eslint \"**/*.js\"",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "mutation-test": "npx stryker run"
  },
  "keywords": [],
